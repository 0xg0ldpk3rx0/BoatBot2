{
	"apps": [
		{
			"name": "v2apidev",
			"script": "./api/main.js",
			"cwd": "./api/",
			"watch": false,
			"min_uptime": 3000,
			"max_restarts": 15,
			"restart_delay": 2500,
			"env": {
				"NODE_ENV": "development",
				"DEBUG": true
			},
			"node_args": "--trace-deprecation",
			"args": "--config=devconfig.json"
		}
	]
}
